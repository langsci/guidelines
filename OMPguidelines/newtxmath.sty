\def\re@DeclareMathSymbol#1#2#3#4{%
    \let#1=\undefined
    \DeclareMathSymbol{#1}{#2}{#3}{#4}}

\def\re@DeclareMathDelimiter#1#2#3#4#5#6{%
    \let#1=\undefined
    \DeclareMathDelimiter{#1}{#2}{#3}{#4}{#5}{#6}}

\def\re@DeclareMathAccent#1#2#3#4{%
    \let#1=\undefined
    \DeclareMathAccent{#1}{#2}{#3}{#4}}

\def\DeclareMathSymbolCtr#1#2#3#4{%
   \DeclareMathSymbol{#1}{#2}{#3}{#4}
   \advance #4 by1}

\def\tx@isdigit#1{% #1 should be a single character
\tx@Isdigit=\z@ %
\count0=\number`#1\relax% 
\ifnum\count0>47\relax% 
\ifnum\count0<58\relax% 
\tx@Isdigit=\@ne % 
\fi% 
\fi%
}

\def\tx@XXX#1#2\@nil{% #1=first character, #2=rest of string
\tx@isdigit{#1}%
\ifnum\tx@Isdigit>\z@ $#1$\else#1\fi%
\if\relax\detokenize{#2}\relax \else
\edef\tx@XXXcmd{\noexpand\tx@XXX #2\noexpand\@nil}\tx@XXXcmd \fi}

\def\equalht#1{\tx@XXX #1\@nil}


